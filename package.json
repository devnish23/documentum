{
  "name": "smart-grocery",
  "version": "1.0.0",
  "description": "Smart Grocery Inventory Manager - Family-focused grocery management with barcode scanning and collaborative features",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace SmartGrocery.xcworkspace -scheme SmartGrocery -configuration Release",
    "clean": "react-native clean",
    "pod:install": "cd ios && pod install",
    "metro:reset": "npx react-native start --reset-cache",
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "backend:build": "cd backend && npm run build",
    "deploy:backend": "cd backend && npm run deploy",
    "deploy:app": "npm run build:android && npm run build:ios",
    "setup": "npm install && cd backend && npm install && cd ../ios && pod install",
    "dev": "concurrently \"npm run start\" \"npm run backend:dev\"",
    "test:e2e": "detox test -c ios.sim.debug",
    "test:e2e:android": "detox test -c android.emu.debug"
  },
  "keywords": [
    "react-native",
    "grocery",
    "inventory",
    "barcode",
    "family",
    "collaboration",
    "multilingual",
    "tamil",
    "typescript"
  ],
  "author": "Smart Grocery Team",
  "license": "MIT",
